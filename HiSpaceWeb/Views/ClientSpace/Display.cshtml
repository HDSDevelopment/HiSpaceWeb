@model HiSpaceWeb.ViewModel.ClientSpaceViewModel

@{
	ViewBag.Title = "Space";
	Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- begin app-main --> <div class="app-main" id="main">
	<!-- begin container-fluid -->
	<div class="container-fluid">
		<!-- begin row -->
		<div class="row">
			<div class="col-md-12 m-b-20">
				<!-- begin page title -->
				<div class="d-block d-sm-flex flex-nowrap align-items-center">
					<div class="page-title mb-2 mb-sm-0">
						<h1>Client Space<span class="action-text"> - Display</span></h1>
					</div>
					<div class="cr-floor">Step 3. <span>Insert Workspace <i class="fa fa-pie-chart" aria-hidden="true"></i></span> </div>
					<div class="ml-auto d-flex align-items-center">
						<nav>
							<ol class="breadcrumb p-0 m-b-0">
								<li class="breadcrumb-item">
									<a href="@Url.Action( "Index","Home")"><i class="ti ti-home"></i></a>
								</li>
								<li class="breadcrumb-item">
									<a href="@Url.Action( "Index","Client")">Client</a>
								</li>
								<li class="breadcrumb-item">
									<a href="@Url.Action( "Index","ClientLocation")">Client Location</a>
								</li>
								<li class="breadcrumb-item">
									<a href="@Url.Action( "Index","ClientSpace", new { ClientLocationID = ViewBag.ClientLocationID})">Client Space</a>
								</li>
								<li class="breadcrumb-item active text-primary" aria-current="page">Display</li>
							</ol>
						</nav>
					</div>
				</div>
				<!-- end page title -->
				<!--Form wizard start-->
				<div class="wizard">
					<div class="wizard-inner">
						<div class=""></div>
						<ul class="nav nav-tabs" role="tablist">

							<li role="presentation" class="">
								<a href="JavaScript:Void(0);">
									<span class="round-tab">
										<i class="fa fa-map-marker" aria-hidden="true"></i>
									</span>
								</a>
								<div class="wz-location">Location</div>
							</li>

							<li role="presentation" class="">
								<a href="JavaScript:Void(0);">
									<span class="round-tab">
										<i class="fa fa-building" aria-hidden="true"></i>
									</span>
								</a>
								<div class="wz-location">Floor</div>
							</li>
							<li role="presentation" class="active">
								<a href="JavaScript:Void(0);">
									<span class="round-tab">
										<i class="fa fa-pie-chart" aria-hidden="true"></i>
									</span>
								</a>
								<div class="wz-location">Space</div>
							</li>

						</ul>
					</div>
				</div>
				<!--Form wizard end-->
			</div>
		</div>
		<!-- end row -->
		<!--Space table Add view-->
		<div class="row" id="Display_view">
			<div class="col-12 mb-3">
				<p><a href="@Url.Action( "Index","ClientSpace", new { ClientLocationID = ViewBag.ClientLocationID})" class="go-to-table"> <i class="fa fa-angle-left" aria-hidden="true"></i> <span>Go Back</span></a></p>
			</div>
			<div class="col-md-12">
				<!--section 1-->
				<div class="card card-statistics">
					<div class="card-header">
						<div class="card-heading">
							<h4 class="card-title">Space Info</h4>
						</div>
					</div>
					<div class="card-body">
						<!--row 1-->
						<div class="row">
							@*@Html.HiddenFor(m => m.ClientSpaceFloorPlan.ClientSpaceFloorPlanID, new { @Value = Model.ClientSpaceFloorPlan.ClientSpaceFloorPlanID })*@
							<div class="col-lg-6 col-md-6 col-sm-6 col-12">
								<div class="form-group">
									@Html.HiddenFor(m => m.ClientSpaceFloorPlan.ClientLocationID)
									@Html.HiddenFor(m => m.ClientSpaceFloorPlan.ClientFloorID)
									@Html.HiddenFor(m => m.ClientSpaceFloorPlan.ClientSpaceFloorPlanID)
									@Html.LabelFor(m => m.ClientSpaceFloorPlan.SpaceName, new { @class = "control-label" })
									@Html.DisplayFor(m => m.ClientSpaceFloorPlan.SpaceName, new { htmlAttributes = new { @placeholder = "Enter Space Name", @class = "" } })
									<i class="bar"></i>
								</div>
							</div>
							<div class="col-lg-6 col-md-6 col-sm-6 col-12">
								<div class="form-group">
									@Html.LabelFor(m => m.ClientSpaceFloorPlan.WSpaceTypeID, new { @class = "control-label" })
									<i class="bar"></i>
									<select disabled id="WSpaceTypeID" name="WSpaceTypeID" asp-for="WSpaceTypeID"
											asp-items="@(new SelectList(ViewBag.ListOfSpaceTypes,"WSpaceTypeID", "WSpaceTypeName"))"></select>
								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-lg-6 col-md-6 col-sm-6 col-12">
								<div class="form-group">
									<label class="control-label">Floor Number</label>
									<i class="bar"></i>
									<select disabled id="ClientFloorID" name="ClientFloorID" asp-for="ClientFloorID"
											asp-items="@(new SelectList(ViewBag.ListOfFloors,"ClientFloorID", "FloorNumber"))"></select>
								</div>
							</div>
							<div class="col-lg-6 col-md-6 col-sm-6 col-12">
								<div class="form-group">
									<label for="StatusName" class="control-label">Status</label><i class="bar"></i>
									<select disabled id="StatusName" name="StatusName" asp-for="StatusName"
											asp-items="@(new SelectList(ViewBag.ListOfStatus,"StatusName", "StatusName"))"></select>

								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-lg-12 col-md-6 col-sm-6 col-12">
								<div class="form-group">
									@Html.LabelFor(m => m.ClientSpaceFloorPlan.SpaceDescription, new { @class = "control-label" })

									@Html.DisplayFor(m => m.ClientSpaceFloorPlan.SpaceDescription, new { htmlAttributes = new { @placeholder = "Enter text..." } })
								</div>
							</div>
						</div>
					</div>
				</div>
				<!--section 2-->
				<div class="card card-statistics">
